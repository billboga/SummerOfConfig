@{
    ViewData["Title"] = "Home Page";
}

@section head
{
    <meta http-equiv="refresh" content="1">
}

<div class="row">
    <div class="col-md-12">
        <div id="progress-distance" class="progress">
            <div class="progress-bar progress-bar-info" style="width: @(ViewBag.Progress)%">
                <div id="progress-distance-text">@(ViewBag.Progress)% (±@(ViewBag.Accuracy)m)</div>
            </div>
        </div>
    </div>
</div>
